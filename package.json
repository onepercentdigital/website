{
  "name": "seo-website",
  "private": true,
  "repository": "https://github.com/onepercentdigital/seo-website.git",
  "packageManager": "bun@1.3.3",
  "type": "module",
  "scripts": {
    "dev": "NODE_OPTIONS='--import ./instrument.server.mjs' vite dev --port 3000",
    "build": "vite build && cp instrument.server.mjs dist/server && tsc --noEmit",
    "serve": "vite preview",
    "test": "vitest run",
    "format": "biome format --write",
    "lint": "biome lint",
    "check": "biome check",
    "deploy": "wrangler deploy",
    "start": "node --import ./dist/server/instrument.server.mjs dist/server/index.js",
    "type": "tsc --noEmit",
    "cf-typegen": "wrangler types",
    "lint-fix": "biome check --write",
    "lint-fix-unsafe": "biome check --write --unsafe",
    "check-all": "tsc --noEmit && biome check"
  },
