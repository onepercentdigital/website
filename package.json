{
  "name": "seo-website",
  "private": true,
  "sideEffects": false,
  "repository": "https://github.com/onepercentdigital/seo-website.git",
  "packageManager": "bun@1.3.2",
  "type": "module",
  "scripts": {
    "dev": "vite dev --port 3000",
    "build": "vite build && tsc --noEmit",
    "deploy": "bun --bun run build && wrangler deploy",
    "preview": "bun --bun run build && vite preview",
    "cf-typegen": "wrangler types",
    "serve": "vite preview",
    "test": "vitest run",
    "format": "biome format --write",
    "lint": "biome lint",
    "lint-fix": "biome check --write",
    "lint-fix-unsafe": "biome check --write --unsafe",
    "check": "biome check",
    "check:all": "bun run type && bun run check",
    "type": "tsc --noEmit"
  },
  "dependencies": {
    "@cloudflare/vite-plugin": "^1.14.2",
    "@radix-ui/react-label": "^2.1.8",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-slider": "^1.3.6",
    "@radix-ui/react-slot": "^1.2.4",
    "@radix-ui/react-switch": "^1.2.6",
    "@tailwindcss/vite": "^4.1.17",
    "@tanstack/react-devtools": "^0.8.1",
    "@tanstack/react-form": "^1.25.0",
    "@tanstack/react-query": "^5.90.10",
    "@tanstack/react-query-devtools": "^5.90.2",
    "@tanstack/react-router": "^1.136.8",
    "@tanstack/react-router-devtools": "^1.136.8",
    "@tanstack/react-router-ssr-query": "^1.136.8",
    "@tanstack/react-start": "^1.136.8",
    "@tanstack/router-plugin": "^1.136.8",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "lucide-react": "^0.553.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "tailwind-merge": "^3.4.0",
    "tailwindcss": "^4.1.17",
    "tw-animate-css": "^1.4.0",
    "vite-tsconfig-paths": "^5.1.4",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.6",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/react": "^16.3.0",
    "@types/node": "^22.19.1",
    "@types/react": "^19.2.5",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.1",
    "jsdom": "^27.2.0",
    "typescript": "^5.9.3",
    "vite": "^7.2.2",
    "vitest": "^4.0.10",
    "web-vitals": "^5.1.0",
    "wrangler": "^4.48.0"
  }
}
